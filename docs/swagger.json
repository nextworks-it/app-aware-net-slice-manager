            }
          ]
        },
        "vasConfiguration": {
          "description": "Vertical Application Slice Configuration",
          "allOf": [
            {
              "$ref": "#/definitions/Intent"
            }
          ]
        },
        "nestId": {
          "type": "string"
        }
      },
      "type": "object",
      "additionalProperties": false
    },
    "Vertical Application Slice Status": {
      "required": [
        "status",
        "vasi"
      ],
      "properties": {
        "vasi": {
          "type": "string"
        },
        "status": {
          "type": "string",
          "example": "INSTANTIATING",
          "enum": [
            "INSTANTIATING",
            "INSTANTIATED",
            "FAILED",
            "TERMINATING",
            "TERMINATED"
          ]
        }
      },
      "type": "object",
      "additionalProperties": false
    },
    "K8s Config": {
      "required": [
        "apiVersion",
        "clusters",
        "contexts",
        "current-context",
        "kind",
        "preferences",
        "users"
