        "status": {
          "type": "string",
          "example": "INSTANTIATING",
          "enum": [
            "INSTANTIATING",
            "INSTANTIATED",
            "FAILED",
            "TERMINATING",
            "TERMINATED"
          ]
        }
      },
      "type": "object",
      "additionalProperties": false
