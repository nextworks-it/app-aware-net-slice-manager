  "info": {
    "title": "Application-Aware Network Slice Manager",
    "version": "1.0",
    "description": "Application-Aware Network Slice Manager LCM APIs."
  },
  "produces": [
    "application/json"
  ],
  "consumes": [
    "application/json"
  ],
  "tags": [
    {
      "name": "lcm/instances",
      "description": "Application-Aware NSM LCM APIs"
    },
    {
      "name": "location",
      "description": "Application-Aware NSM Location APIs"
    }
  ],
  "definitions": {
    "Intent": {
      "required": [
        "computingConstraints",
        "locationConstraints",
        "networkingConstraints"
      ],
      "properties": {
        "locationConstraints": {
          "type": "array",
          "description": "List of Geographical Area Identifiers",
          "items": {
            "$ref": "#/definitions/Location Constraint"
          }
        },
        "computingConstraints": {
          "type": "array",
          "description": "List of Computing Constraints",
          "items": {
            "$ref": "#/definitions/Computing Constraint"
          }
        },
        "networkingConstraints": {
          "type": "array",
          "description": "List of Networking Constraints",
          "items": {
            "$ref": "#/definitions/Networking Constraint"
          }
        }
      },
      "type": "object",
      "additionalProperties": false
    },
    "Location Constraint": {
      "required": [
        "geographicalAreaId"
      ],
      "properties": {
        "geographicalAreaId": {
          "type": "string"
        }
      },
      "type": "object",
      "additionalProperties": false
    },
    "Computing Constraint": {
      "required": [
        "applicationComponentId",
        "cpu",
        "ram",
        "storage"
      ],
      "properties": {
        "applicationComponentId": {
          "type": "string"
        },
        "ram": {
          "type": "string"
        },
        "cpu": {
          "type": "string"
        },
        "storage": {
          "type": "string"
        }
      },
      "type": "object",
      "additionalProperties": false
    },
    "Networking Constraint": {
      "required": [
        "applicationComponentEndpointId",
        "applicationComponentId",
        "sliceProfiles"
      ],
      "properties": {
        "applicationComponentId": {
          "type": "string"
        },
        "applicationComponentEndpointId": {
          "type": "string"
        },
        "sliceProfiles": {
          "type": "array",
          "description": "List of Slice Profiles",
          "items": {
            "$ref": "#/definitions/Slice Profile"
          }
        }
      },
      "type": "object",
      "additionalProperties": false
    },
    "Slice Profile": {
      "required": [
        "profileParams",
        "sliceType"
      ],
      "properties": {
        "sliceType": {
          "type": "string",
          "example": "EMBB",
          "enum": [
            "EMBB",
            "URLLC",
            "MMTC"
          ]
        },
        "profileParams": {
          "description": "Slice Profile Parameters",
          "allOf": [
            {
              "$ref": "#/definitions/Profile Params"
            }
          ]
        }
      },
      "type": "object",
      "additionalProperties": false
    },
    "Profile Params": {
      "required": [
        "availability",
        "errorRate",
        "isolationLevel",
        "maximumNumberUE",
        "uEDensity",
        "uESpeed"
      ],
      "properties": {
        "availability": {
          "type": "number"
        },
        "errorRate": {
          "type": "number"
        },
        "isolationLevel": {
          "type": "string",
          "example": "NO_ISOLATION",
          "enum": [
            "NO_ISOLATION",
            "LOGICAL"
          ]
        },
        "maximumNumberUE": {
          "type": "integer"
        },
        "uESpeed": {
          "type": "number"
        },
        "uEDensity": {
          "type": "number"
        },
        "ulThroughput": {
          "type": "number"
        },
        "dlThroughput": {
          "type": "number"
        },
        "ulThroughputUE": {
          "type": "number"
        },
        "dlThroughputUE": {
          "type": "number"
        },
        "dataRate": {
          "type": "number"
        },
        "delay": {
          "type": "number"
        },
        "jitter": {
          "type": "number"
        },
        "priorityLevel": {
          "type": "integer"
        }
      },
      "type": "object",
      "additionalProperties": false
    },
    "Error Message": {
      "required": [
        "message"
      ],
      "properties": {
        "message": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "Vertical Application Slice Status Information": {
      "required": [
        "nestId",
        "networkSliceStatus",
        "vaQuotaInfo",
        "vasConfiguration",
        "vasStatus"
      ],
      "properties": {
        "vasStatus": {
          "description": "Vertical Application Slice Status",
          "allOf": [
            {
              "$ref": "#/definitions/Vertical Application Slice Status"
            }
          ]
        },
        "vaQuotaInfo": {
          "description": "Vertical Application Quota Information",
