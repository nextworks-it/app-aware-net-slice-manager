          }
        }
      },
      "type": "object",
      "additionalProperties": false
    },
    "slice_profile": {
      "required": [
        "profileParams",
        "sliceType"
      ],
      "properties": {
        "sliceType": {
          "type": "string",
          "example": "EMBB",
          "enum": [
            "EMBB",
            "URLLC",
            "MMTC"
          ]
        },
        "profileParams": {
          "description": "Slice Profile Parameters",
          "allOf": [
            {
              "$ref": "#/definitions/profile_params"
            }
          ]
        }
      },
      "type": "object",
      "additionalProperties": false
    },
    "profile_params": {
      "required": [
        "availability",
        "errorRate",
        "isolationLevel",
        "maximumNumberUE",
        "uEDensity",
        "uESpeed"
      ],
      "properties": {
        "availability": {
          "type": "number"
        },
        "errorRate": {
          "type": "number"
        },
        "isolationLevel": {
          "type": "string",
          "example": "NO_ISOLATION",
          "enum": [
            "NO_ISOLATION",
            "LOGICAL"
          ]
        },
        "maximumNumberUE": {
          "type": "integer"
        },
        "uESpeed": {
          "type": "number"
        },
        "uEDensity": {
          "type": "number"
        },
        "ulThroughput": {
          "type": "number"
        },
        "dlThroughput": {
          "type": "number"
        },
        "ulThroughputUE": {
          "type": "number"
        },
        "dlThroughputUE": {
          "type": "number"
        },
        "dataRate": {
          "type": "number"
        },
        "delay": {
          "type": "number"
        },
        "jitter": {
          "type": "number"
        },
        "priorityLevel": {
          "type": "integer"
        }
      },
      "type": "object",
      "additionalProperties": false
    },
    "error_msg": {
      "required": [
        "message"
      ],
      "properties": {
        "message": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "vas_info": {
      "required": [
        "nestId",
        "networkSliceStatus",
        "vaQuotaInfo",
        "vasConfiguration",
        "vasStatus"
